{
  "name": "node-redux-auth",
  "version": "1.0.0",
  "description": "Token authentication system using Node, Mongo, React, Redux",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack/webpack-dev.config.js --watch --colors",
    "build": "rm -rf dist && webpack --config ./webpack/webpack-prod.config.js --colors",
    "start": "NODE_ENV=production PORT=3000 pm2 start ./server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "React",
    "Redux",
    "Token Authentication",
    "Airbnb Eslint",
    "SCSS",
    "Babel",
    "Webpack Configuration"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimitriMikadze/node-redux-auth"
  },
  "author": "Dimitri Mikadze",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.16.1",
    "jquery": "^3.1.0",
    "jsdom": "^9.0.0",
    "mocha": "^3.0.2",
    "node-sass": "^4.14.1",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "express": "^4.13.4",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2",
    "redux-form": "^6.0.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0"
  }
}
